{
	"masterPicture": "TODO: Path to your master picture",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "noChange",
			"assets": {
				"ios6AndPriorIcons": true,
				"ios7AndLaterIcons": true,
				"precomposedIcons": true,
				"declareOnlyDefaultIcon": true
			},
			"appName": "MyAdmin panel on React"
		},
		"desktopBrowser": {},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#434a54",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": true,
				"windows10Ie11EdgeTiles": {
					"small": true,
					"medium": true,
					"big": true,
					"rectangle": true
				}
			},
			"appName": "MyAdmin panel on React"
		},
		"androidChrome": {
			"pictureAspect": "noChange",
			"themeColor": "#434a54",
			"manifest": {
				"name": "MyAdmin panel on React",
				"startUrl": "https://github.com/mrswylet/myadmin",
				"display": "standalone",
				"orientation": "portrait",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": true,
				"lowResolutionIcons": true
			}
		}
	},
	"settings": {
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": true,
		"htmlCodeFile": true,
		"usePathAsIs": false
	}
}